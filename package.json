{
  "name": "@hcl/capire-hcl-dataeagle",
  "version": "1.0.0",
  "description": "HCL Dataeagle Solution (REL20210809)",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5.4.0",
    "express": "^4",
    "hdb": "^0.18.3",
    "mbt": "^1.2.2"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.90.6",
    "sqlite3": "^5.0.2"
  },
  "scripts": {
    "start": "cds run"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "model": "*",
        "credentials": {
          "database": "db/variant.db"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "sapux": [
    "app/dataeagle-managevariant",
    "app/dataeagle-manageuservariant"
  ]
}